    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Book to speech",
            "request": "launch",
            "mainClass": "lilytts.App",
            "projectName": "lilytts",
            "args": [
                "book-to-speech-azure",
                "/Users/lily/Library/CloudStorage/OneDrive-Personal/Book workspace/Chapters",
                "/Users/lily/Library/CloudStorage/OneDrive-Personal/Book workspace/Audio",
                "--only",
                "/Users/lily/Library/CloudStorage/OneDrive-Personal/Book workspace/Chapters/Preface.txt",
                "@/Users/lily/Library/CloudStorage/OneDrive-Personal/Book workspace/arguments.args"
            ]
        },
        {
            "type": "java",
            "name": "Launch SSML",
            "request": "launch",
            "mainClass": "lilytts.App",
            "projectName": "lilytts",
            "args": "text-to-ssml @/Users/lily/Downloads/ssml-argfile.args"
        },
        {
            "type": "java",
            "name": "Launch Merge Lines",
            "request": "launch",
            "mainClass": "lilytts.App",
            "projectName": "lilytts",
            "args": "merge-lines @/Users/lily/Downloads/merge-lines-argfile.args"
        },
        {
            "type": "java",
            "name": "Launch News to speech",
            "request": "launch",
            "mainClass": "lilytts.App",
            "projectName": "lilytts",
            "args": "news-to-speech-azure @/Users/lily/Documents/Articles/arguments.args"
        }
    ]
}